{
  "manifest": {
    "publishingInformation": {
      "locales": {
        "en-US": {
          "summary": "This is the official FIFA Alexa skill.",
          "examplePhrases": [
            "alexa, open fifa",
            "alexa, ask fifa"
          ],
          "keywords": [
            "fifa",
            "Football World Cup",
            "Woman's World Cup France 2019"
          ],
          "name": "fifa",
          "description": "This skills keeps you up to date with FIFA World Cups."
        }
      },
      "isAvailableWorldwide": false,
      "testingInstructions": "1) Say 'Alexa, ask FIFA when Switzerland is playing?'",

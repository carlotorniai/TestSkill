      "distributionCountries": [
        "US",
        "GB",
        "FR",
        "CH",
        "DE"
      ]
    },
    "apis": {
      "custom": {
        "endpoint": {
          "uri": "arn:aws:lambda:us-east-1:040623927470:function:sampleSkill"
        },
        "interfaces": [
          {
            "type":"CAN_FULFILL_INTENT_REQUEST"
          }
        ]
      }
    },
    "manifestVersion": "1.0",
    "permissions": [
      {
        "name": "alexa::alerts:reminders:skill:readwrite"
      }
